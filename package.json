{
  "name": "roopiy_ui",
  "version": "1.0.0",
  "main": "./build/electron/Main.js",
  "license": "MIT",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --config webpack.config.js && electron --noDevServer .",
    "dev": "node scripts/dev.js",
    "dev:electron-react": "cross-env-shell NODE_ENV=development FLAVOR=electron webpack-dev-server --config webpack.react.js --mode=development",
    "dev:electron": "cross-env-shell NODE_ENV=development webpack --config config/webpack/webpack.electron.js --mode=development --env port=${PORT} && npm run start:electron",
    "dev:electron:webpack": "cross-env-shell NODE_ENV=development webpack --config config/webpack/webpack.electron.js --mode=development",
    "build": "npm-run-all build:electron build:react",
    "build:electron": "webpack --config webpack/electron.webpack.js --mode=production",
    "build:react": "webpack --config webpack/react.webpack.ts --mode=production",
    "build:run": "npm-run-all build start:electron",
    "start:electron": "electron .",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir",
    "lint": "npx eslint src/**/**.{js,jsx,ts,tsx}",
    "stats": "npx webpack --profile --json build/compilation-stats.json && npx webpack-bundle-analyzer build/compilation-stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-utils": "^0.13.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-root-import": "^6.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "electron": "^26.3.0",
    "eslint": "^8.0.1",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-webpack-plugin": "^4.0.0",
    "favicons": "^7.1.4",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "img-loader": "^4.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "process": "^0.11.10",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "*",
    "url-loader": "^4.1.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.58.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.3.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.131",
    "@mui/material": "^5.13.1",
    "@uiw/react-md-editor": "^3.23.3",
    "array-move": "^4.0.0",
    "classnames": "^2.3.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-css-component": "^1.0.2",
    "react-dom": "^18.2.0",
    "react-easy-sort": "^1.5.1",
    "react-helmet-async": "^1.3.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.11.1",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "react-toastify": "^9.1.3"
  }
